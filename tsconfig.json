{
  "extends": "@bluelovers/tsconfig",
  "compilerOptions": {
    "lib": [
      "esnext",
      "webworker"
    ],
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["lib"],
  "exclude": ["node_modules", "*.spec.ts"]
}
